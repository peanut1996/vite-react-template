    "tsm": "tsm src -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "antd": "^4.17.2",
    "axios": "^0.24.0",
    "better-mock": "^0.3.2",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "store2": "^2.12.0",
    "urijs": "^1.19.7"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.7",
